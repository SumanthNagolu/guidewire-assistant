{
  "name": "intime-academy-lms",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "test": "pnpm test:unit && pnpm test:integration",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:perf": "k6 run tests/performance/load-test.ts",
    "test:load": "k6 run --vus 100 --duration 30m tests/performance/load-test.ts",
    "test:coverage": "sh scripts/test-coverage.sh",
    "test:e2e:ui": "playwright test --ui",
    "db:generate": "supabase gen types typescript --local > types/database.generated.ts",
    "db:migrate": "tsx scripts/migrate-to-master-schema.ts",
    "db:migrate:prod": "tsx scripts/migrate-to-master-schema.ts --production",
    "db:rollback": "psql $DATABASE_URL -f database/migration-rollback.sql",
    "db:verify": "tsx scripts/verify-migration.ts",
    "db:backup": "tsx scripts/backup-database.ts",
    "db:seed": "tsx scripts/seed.ts",
    "hr:init": "tsx scripts/init-hr-db.ts",
    "smtp:setup": "node scripts/setup-smtp.js",
    "smtp:test": "node scripts/test-smtp.js",
    "smtp:verify": "node scripts/verify-smtp-config.js",
    "smtp:check": "node scripts/check-smtp-status.js",
    "smtp:cleanup": "node scripts/cleanup-failed-invites.js",
    "analyze": "ANALYZE=true pnpm build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
