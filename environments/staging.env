# Staging Environment Configuration
# DO NOT COMMIT WITH REAL VALUES

# Supabase (Staging)
NEXT_PUBLIC_SUPABASE_URL=https://your-staging-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-staging-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-staging-service-role-key

# OpenAI
OPENAI_API_KEY=your-openai-api-key

# Anthropic
ANTHROPIC_API_KEY=your-anthropic-api-key

# Email (SendGrid)
SENDGRID_API_KEY=your-sendgrid-api-key
SENDGRID_FROM_EMAIL=staging@intimesolutions.com

# Sentry (Same as production)
NEXT_PUBLIC_SENTRY_DSN=your-sentry-dsn
SENTRY_ORG=your-sentry-org
SENTRY_PROJECT=your-sentry-project
SENTRY_AUTH_TOKEN=your-sentry-auth-token

# Google Analytics (Staging)
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-STAGING123

# Redis (Upstash - Staging)
UPSTASH_REDIS_REST_URL=https://your-staging-redis.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-staging-redis-token

# Feature Flags
NEXT_PUBLIC_ENABLE_AI_MENTOR=true
NEXT_PUBLIC_ENABLE_PRODUCTIVITY=true
NEXT_PUBLIC_ENABLE_GAMIFICATION=true
NEXT_PUBLIC_ENABLE_CEO_DASHBOARD=false

# Environment
NODE_ENV=staging
NEXT_PUBLIC_ENVIRONMENT=staging
